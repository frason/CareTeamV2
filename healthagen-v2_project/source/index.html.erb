<section id="options" class="clearfix"> 
  <div class="option-combo">
    <h3>Filter by:</h3>
    <ul id="filters" class="option-set clearfix" data-option-key="filter">
      <li><a href="#filter" data-option-value="*" class="selected">All</a></li>
      <li><a href="#filter" data-option-value=".qualityMeasures">Quality Measures</a></li>
      <li><a href="#filter" data-option-value=".utilMeasures">Utlization Measures</a></li>
      <li><a href="#filter" data-option-value=".registry">Registry</a></li>
    </ul>
  </div>

  <!--div class="option-combo option-right">
    <h3>Sort order:</h3>
