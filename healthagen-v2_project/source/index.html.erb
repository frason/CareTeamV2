<link rel="stylesheet" href="css/isotope-style.css">


<section id="options" class="clearfix"> 
  <div class="option-combo">
    <h3>Filter by:</h3>
    <ul id="filters" class="option-set clearfix" data-option-key="filter">
      <li><a href="#filter" data-option-value="*" class="selected">All</a></li>
      <li><a href="#filter" data-option-value=".qualityMeasures">Quality Measures</a></li>
      <li><a href="#filter" data-option-value=".utilMeasures">Utlization Measures</a></li>
      <li><a href="#filter" data-option-value=".registry">Registry</a></li>
    </ul>
  </div>

  <div class="option-combo option-right">
    <h3>Sort order:</h3>
    <ul id="sort-direction" class="option-set clearfix" data-option-key="sortAscending">
      <li><a href="#sortAscending=true" data-option-value="true" class="selected">Ascending</a></li>
      <li><a href="#sortAscending=false" data-option-value="false">Descending</a></li>
    </ul>
  </div>

  <div class="option-combo option-right">
    <h3>Sort by:</h3>
    <ul id="sort-by" class="option-set clearfix" data-option-key="sortBy">
      <li><a href="#sortBy=original-order" data-option-value="original-order" class="selected" data>Default</a></li>
      <li><a href="#sortBy=date" data-option-value="date">Date</a></li>
      <li><a href="#sortBy=name" data-option-value="name">Name</a></li>
      <li><a href="#sortBy=number" data-option-value="number">Percentage</a></li>
    </ul>
  </div>
</section><!-- #options -->

