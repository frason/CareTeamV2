---
title: Dashboard
---

<div class="row clearfix">  
  <div class="col col12-span6 col8-span4">
    <div class="ribbon">
      <i class="icon-check"></i>
    </div>
    people count: <%= data.people.friends.count %>
    <ul>
      <% data.people.friends.each_with_index do |f, index| %>
      <li><%= index + 1 %>: <%= f %></li>
      <% end %>
    </ul>
  </div>

  <div class="col col12-span6 col8-span4 omega">
    sidebar count: <%= data.sidebar.count %>
    <ul>
      <% 1.upto(5) do |num| %>
          <li><%= num %>: <%= data.sidebar[num] %></li>
      <% end %>
    </ul>
  </div>

  <!--div class="clearfix noshow visible-col8"></div--> 

  <div class="col col12-span3 col8-span2">
  </div>

  <div class="col col12-span3 col8-span2">
  </div>

  <div class="col col12-span3 col8-span2">
  </div>

  <div class="col col12-span3 col8-span2 omega">
  </div>
</div><!-- .row -->

<div class="row clearfix">
  <div class="col col12-span12 col8-span8">
  </div>
</div><!-- .row -->


